---
const { title, date } = Astro.props;
---

<!doctype html>
<html lang="ja">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
    <link rel="stylesheet" href="https://use.typekit.net/pvb3tuh.css" />
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/1.4.0/p5.js"
      is:inline></script>
    <link rel="stylesheet" href="/styles/sketch.css" />
    <slot name="head" />
  </head>
  <body>
    <h1>{title}</h1>
    <div id="p5_loading" class="loadingclass">loading...loading...loading...loading...loading...loading...loading...loading...</div>
    <slot />
  </body>
</html>
