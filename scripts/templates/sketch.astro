---
import SketchLayout from "../../layouts/SketchLayout.astro";
import "../styles/sketch.scss";
const title = "{{title}}";
const date = "{{date}}";
---

<SketchLayout title={title} date={date}>
  <link slot="head" rel="stylesheet" href={`/${date}/style.css`} />
  <script slot="head" src={`/${date}/script.js`} is:inline></script>
</SketchLayout>
